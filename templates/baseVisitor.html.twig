        </div>
        {% block javascripts %}{% endblock %}
        <script src="js/slideshow.js"></script>
        <script>
            Slideshow.init({
                container:{
                    width:700,
                    height:350
                }
            });
        </script>
        <script src="js/titleshow.js"></script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCWZY4wrhaBgenM_t79GTQva2cCcUG3YOM&callback=MaMap.initMap" async defer></script>
        <script src="js/map.js"></script>
