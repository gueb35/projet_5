                            <li class="nav-item">
                                <a class="nav-link" href="{{ path('security_registration_bask_coll') }}">Inscription aux paniers collectés</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="{{ path('security_login_members') }}">Espace membres</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="{{ path('security_login_admin') }}">Espace administrateur</a>
                            </li>
                        </ul>
                    </div>
            </nav>
